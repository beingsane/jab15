<?xml version="1.0" encoding="UTF-8"?>
<fof>
    <common>
        <container>
            <option name="componentNamespace">Akeeba\JAB15</option>
            <option name="factoryClass">FOF30\Factory\MagicFactory</option>
        </container>

        <dispatcher>
            <option name="defaultView">conferences</option>
        </dispatcher>

        <model name="*">
            <behaviors merge="1">Filters</behaviors>
        </model>
    </common>

    <frontend>
        <model name="Conferences">
            <behaviors merge="1">Enabled</behaviors>
        </model>
        <model name="Sessions">
            <behaviors merge="1">Enabled,PageParametersToState</behaviors>
        </model>
        <model name="Attendances">
            <behaviors merge="1">Own</behaviors>
        </model>
    </frontend>
</fof>